<h1>This is the home</h1>
<a href="/create">Create Article</a>
<div>
  <% articles.forEach(function(article){ %>
  <div>
    <!-- _id -->
    <a href="/article/<%= article._id %>"><h3><%= article.title %></h3></a>
    <div>Created on: <%= moment(article.createdAt).fromNow() %></div>
    <div><a href="/article/<%= article._id %>/delete">Delete</a></div>
  </div>
  <% }) %>
</div>
